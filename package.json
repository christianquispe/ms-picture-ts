{
  "name": "ms-picture-ts",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:christianquispe/ms-picture-ts.git",
  "author": "christianquispe <christianquispecamasca@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon app.ts",
    "build-ts": "tsc",
    "build": "npm run build-ts && npm run rsync",
    "rsync": "rsync -a --prune-empty-dirs --include 'server/graphql/*' --include '*.gql' 'server/graphql' 'dist/server'"
  },
  "dependencies": {
    "@graphql-tools/schema": "^8.3.1",
    "@graphql-tools/stitch": "^8.4.2",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "express-winston": "^4.0.5",
    "graphql": "^15.5.0",
    "graphql-import": "^1.0.2",
    "graphql-tools": "^7.0.2",
    "graphql-upload": "^11.0.0",
    "mongoose": "^6.0.14",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/graphql-upload": "^8.0.7",
    "@types/mongoose": "^5.10.3",
    "@types/node": "^14.14.22",
    "nodemon": "^2.0.15",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2"
  }
}